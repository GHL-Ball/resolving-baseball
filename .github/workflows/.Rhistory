# 開始日と終了日の定義
start_date <- ymd("2024-03-20")  # 開始日
end_date <- ymd("2024-10-30")    # 終了日
# 期間を3日ごとに分割してデータを取得する
dates <- seq(start_date, end_date, by = "3 days")
# データを格納するリストを作成
dfs <- list()
error_messages <- c() # エラーメッセージを格納するベクトル
# データを取得してリストに追加
for (i in 1:(length(dates) - 1)) {
start <- dates[i]
end <- dates[i + 1] - days(1)
# tryCatchでエラーハンドリング
tryCatch({
df <- scrape_statcast_savant(start_date = start, end_date = end, player_type = "batter")
dfs[[i]] <- df
}, error = function(e) {
error_messages <<- c(error_messages, paste("データが取得できませんでした:", start, "から", end, "まで"))
})
}
